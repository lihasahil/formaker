generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model JsonForm {
  id          Int      @id @default(autoincrement())
  jsonform    String
  createdBy   String
  createdAt   DateTime @default(now())
  background  String   @default("#ffffff")
  borderWidth Int      @default(2)
}
